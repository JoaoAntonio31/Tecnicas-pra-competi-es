#include <bits/stdc++.h>

using namespace std;
int c,d=0;
int main() {
    string s;
    cin >> s;
    for(int i =0;i<s.size();i++){
        if(s[i]>=97 && s[i]<=122){
            c++;
        }
        else{
            d++;
        }
    }
     if(c<d){
        char ch;
        for(int i=0;i<s.size();i++){
            ch=toupper(s[i]);
            cout << ch;
        }
     }
     else{
        char ch;
        for(int i =0;i<s.size();i++){
            ch = tolower(s[i]);
            cout << ch;
        }
     }

 	return 0;
}
